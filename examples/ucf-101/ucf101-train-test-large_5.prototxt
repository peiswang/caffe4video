  weight_decay: 1
  weight_decay: 0
  recursive_once_param {
    group: 4
    num_uv: 2
    relative_position: 0
    relative_position: 1
    assemble_size: 5
    stride: 2
    weight_filler {
      type: "gaussian"
      std: 0.01
    }
    bias_filler {
