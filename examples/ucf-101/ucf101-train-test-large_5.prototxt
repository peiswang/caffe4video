layers {
  name: "r9"
  type: RECURSIVE_ONCE
  bottom: "r8"
  top: "r9"
  blobs_lr: 1
  blobs_lr: 2
  weight_decay: 1
  weight_decay: 0
  recursive_once_param {
    group: 2
    num_uv: 2
    relative_position: 0
    relative_position: 1
    assemble_size: 5
    stride: 1
    weight_filler {
