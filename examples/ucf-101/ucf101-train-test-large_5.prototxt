name: "conv_rnn_net"
layers {
  name: "data"
  type: DATA
  top: "data"
  top: "label"
  data_param {
    source: "examples/ucf-101/ucf101_train_lmdb"
    backend: LMDB
    batch_size: 8
  } 
  transform_param {
    is_video: true
    video_crop_size_t: 36
    video_crop_size_h: 227
    video_crop_size_w: 227
    mean_file: "examples/ucf-101/ucf101_mean.binaryproto"
    mirror: true
  }
  include: { phase: TRAIN }
}

layers {
  name: "data"
  type: DATA
  top: "data"
  top: "label"
  data_param {
    source: "examples/ucf-101/ucf101_test_lmdb"
    backend: LMDB
    batch_size: 8
  }
  transform_param {
    is_video: true
    video_crop_size_t: 36
    video_crop_size_h: 227
    video_crop_size_w: 227
    mean_file: "examples/ucf-101/ucf101_mean.binaryproto"
    mirror: false 
  }
  include: { phase: TEST }
}

layers {
  name: "conv1"
  type: MULTICONVOLUTION  
  bottom: "data"
  top: "conv1"
  blobs_lr: 1
  blobs_lr: 2
  weight_decay: 1
  weight_decay: 0
  multi_convolution_param {
    num_output: 3456 
    group: 36
    kernel_size: 11
    stride: 4
    weight_filler {
      type: "gaussian"
      std: 0.01 
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}

layers {
  name: "relu1"
  type: RELU
  bottom: "conv1"
  top: "conv1"
}

layers {
  name: "pool1"
  type: POOLING
  bottom: "conv1"
  top: "pool1"
  pooling_param {
    pool: MAX
    kernel_size: 3
    stride: 2
  }
}

layers {
  name: "norm1"
  type: LRN
  bottom: "pool1"
  top: "norm1"
  lrn_param {
    local_size: 5
    alpha: 0.0001
    beta: 0.75
  }
}

layers {
  name: "r1"
  type: RECURSIVE_ONCE
  bottom: "norm1"
  top: "r1"
  blobs_lr: 1
  blobs_lr: 2
  weight_decay: 1
  weight_decay: 0
  recursive_once_param {
    group: 36
    relative_position: 0
    relative_position: 1
    num_uv: 2
    assemble_size: 5
    stride: 2
    weight_filler {
      type: "gaussian"
      std: 0.01
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
  param: "rnn_first_w"
  param: "rnn_first_b"
}

layers {
  name: "relu_r1"
  type: RELU
  bottom: "r1"
  top: "r1"
}

layers {
  name: "r2"
  type: RECURSIVE_ONCE
  bottom: "r1"
  top: "r2"
  blobs_lr: 1
  blobs_lr: 2
  weight_decay: 1
  weight_decay: 0
  recursive_once_param {
    group: 18
    relative_position: 0
    relative_position: 1
    num_uv: 2
    assemble_size: 5
    stride: 1
    weight_filler {
      type: "gaussian"
      std: 0.01
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
  param: "rnn_first_w"
  param: "rnn_first_b"
}

layers {
  name: "relu_r2"
  type: RELU
  bottom: "r2"
  top: "r2"
}

layers {
  name: "r3"
  type: RECURSIVE_ONCE
  bottom: "r2"
  top: "r3"
  blobs_lr: 1
  blobs_lr: 2
  weight_decay: 1
  weight_decay: 0
  recursive_once_param {
    group: 17
    relative_position: 0
    relative_position: 2
    num_uv: 2
    assemble_size: 5
    stride: 1
    weight_filler {
      type: "gaussian"
      std: 0.01
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
  param: "rnn_first_w"
  param: "rnn_first_b"
}

layers {
  name: "relu_r3"
  type: RELU
  bottom: "r3"
  top: "r3"
}

layers {
  name: "conv2"
  type: MULTICONVOLUTION
  bottom: "r3"
  top: "conv2"
  blobs_lr: 1
  blobs_lr: 2
  weight_decay: 1
  weight_decay: 0
  multi_convolution_param {
    num_output: 3840
    group: 15
    pad: 2
    kernel_size: 5
    weight_filler {
      type: "gaussian"
      std: 0.01
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}

layers {
  name: "relu2"
  type: RELU
  bottom: "conv2"
  top: "conv2"
}
layers {
  name: "pool2"
  type: POOLING
  bottom: "conv2"
  top: "pool2"
  pooling_param {
    pool: MAX
    kernel_size: 3
    stride: 2
  }
}
layers {
  name: "norm2"
  type: LRN
  bottom: "pool2"
  top: "norm2"
  lrn_param {
    local_size: 5
    alpha: 0.0001
    beta: 0.75
  }
}

layers {
  name: "r4"
  type: RECURSIVE_ONCE
  bottom: "norm2"
  top: "r4"
  blobs_lr: 1
  blobs_lr: 2
  weight_decay: 1
  weight_decay: 0
  recursive_once_param {
    group: 15
    num_uv: 2
    relative_position: 0
    relative_position: 1
    assemble_size: 5
    stride: 1
    weight_filler {
      type: "gaussian"
      std: 0.01
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
  param: "rnn_second_w"
  param: "rnn_second_b"  
}

layers {
  name: "relu_r4"
  type: RELU
  bottom: "r4"
  top: "r4"
}

layers {
  name: "r5"
  type: RECURSIVE_ONCE
  bottom: "r4"
  top: "r5"
  blobs_lr: 1
  blobs_lr: 2
  weight_decay: 1
  weight_decay: 0
  recursive_once_param {
    group: 14
    num_uv: 2
    relative_position: 0
    relative_position: 2
    assemble_size: 5
    stride: 1
    weight_filler {
      type: "gaussian"
      std: 0.01
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
  param: "rnn_second_w"
  param: "rnn_second_b"  
}

layers {
  name: "relu_r5"
  type: RELU
  bottom: "r5"
  top: "r5"
}

layers {
  name: "r6"
  type: RECURSIVE_ONCE
  bottom: "r5"
  top: "r6"
  blobs_lr: 1
  blobs_lr: 2
  weight_decay: 1
  weight_decay: 0
  recursive_once_param {
    group: 12
    num_uv: 2
    relative_position: 0
    relative_position: 4
    assemble_size: 5
    stride: 1
    weight_filler {
      type: "gaussian"
      std: 0.01
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
  param: "rnn_second_w"
  param: "rnn_second_b"  
}

layers {
  name: "relu_r6"
  type: RELU
  bottom: "r6"
  top: "r6"
}

layers {
  name: "conv3"
  type: MULTICONVOLUTION
  bottom: "r6"
  top: "conv3"
  blobs_lr: 1
  blobs_lr: 2
  weight_decay: 1
  weight_decay: 0
  multi_convolution_param {
    num_output: 3072
    group: 8
    pad: 1
    kernel_size: 3
    weight_filler {
      type: "gaussian"
      std: 0.01
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}
layers {
  name: "relu3"
  type: RELU
  bottom: "conv3"
  top: "conv3"
}

layers {
  name: "pool3"
  type: POOLING
  bottom: "conv3"
  top: "pool3"
  pooling_param {
    pool: MAX
    kernel_size: 3
    stride: 2
  }
}

